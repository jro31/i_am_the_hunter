<h1>Add a new job application</h1>

<div class="form_wrapper">
  <%= simple_form_for(@job) do |f| %>
    <%= f.input :company_name, label: "What is the name of the company that you applied to?", placeholder: "Apple" %>
    <%= f.input :agency, label: "Are they an agency?" %>
    <%= f.submit 'Add job', class: 'btn btn-primary' %>
  <% end %>
</div>
